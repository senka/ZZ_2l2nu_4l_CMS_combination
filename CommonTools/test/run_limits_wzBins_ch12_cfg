python buildWZworkspace_forZZ_1sigma_allErrOK_withEWKSyst.py --config=config_ZZ_Zlldata_withSherpaSyst_withEWKSyst_binBYbin

// using lnN with value in datacard for syst:
python buildWZworkspace_lnNall_noSignalBkgCorrUnc.py --config=config_ZZ_Zlldata_withSherpaSyst_binBYbin
python buildWZworkspace_lnNall_noSignalBkgCorrUnc.py --config=config_ZZ_Zlldata_ch6_largeUncOnly
