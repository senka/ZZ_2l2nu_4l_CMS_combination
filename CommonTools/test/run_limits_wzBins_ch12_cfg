//
//		asymptotic CLs
//

combine -M Asymptotic Exampl* --setPhysicsParameters dkg=0.6 --freeseNuisances dkg --redefineSignalPOIs r
combine -M Asymptotic Example_ATGC_WV_ch1to6_workspace.root --setPhysicsModelParameters dkg=0.002,lZ=0.,dg1=0. --freezeNuisances dkg,lZ,dg1 --redefineSignalPOIs r --verbose 9
combine -M Asymptotic Example_ATGC_WV_ch1to6_workspace.root --setPhysicsModelParameters dkg=0.005 --redefineSignalPOIs r
ok:
combine -M Asymptotic Example_ATGC_WV_ch1to6_workspace.root --setPhysicsModelParameters dkg=0.005,lZ=0.,dg1=0. --freezeNuisances dkg,lZ,dg1 --redefineSignalPOIs r --verbose 9 | tee output_asymptotic_0p005

