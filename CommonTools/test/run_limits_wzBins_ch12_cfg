python buildWZworkspace_lnNall.py --config=config_ZZ_Zlldata_statUncOnly_binBYbin

rm wv_semil_ch1to6combined.txt
combineCards.py wv_semil_ch1boosted.txt wv_semil_ch2boosted.txt wv_semil_ch3boosted.txt wv_semil_ch4boosted.txt wv_semil_ch5boosted.txt wv_semil_ch6boosted.txt > wv_semil_ch1to6combined.txt
